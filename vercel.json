{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "status": 200,
        "headers": {
          "cache-control": "public, max-age=0, must-revalidate"
        },
        "continue": true
      }
    ]
  }
  